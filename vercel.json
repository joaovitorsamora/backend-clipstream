{
    "version": 2,
    "name": "backend-clipstream",
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "outputDirectory": "dist"
  }
  