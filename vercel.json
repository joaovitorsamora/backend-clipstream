{
    "version": 2,
    "name": "backend-clipstream",
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.js"
      }
    ],
    "buildCommand": "npm install && npm run build"
  }
  